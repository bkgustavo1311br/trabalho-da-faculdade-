

// Declaração das variáveis
Início 

inteiro idade
real minutos_vividos
FimAlgoritmo

// Entrada de dados
escreva("Digite a idade da pessoa em anos: ")
leia(idade)

// Cálculo dos minutos vividos
minutos_vividos = idade * 365 * 24 * 60

// Saída de dados
escreva("A pessoa viveu ", minutos_vividos, " minutos.")

FimAlgoritmo
